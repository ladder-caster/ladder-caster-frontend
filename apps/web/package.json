{
  "name": "web",
  "app": "build/index.html",
  "targets": {
    "app": {
      "publicUrl": "./"
    }
  },
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "web": "parcel serve public/index.html",
    "build": "rimraf build && parcel build public/index.html --dist-dir build/ --no-source-maps --no-scope-hoist --no-cache --public-url .",
    "deploy-prod": "build-prod && vc --cwd build && vercel deploy --prebuilt",
    "vercel-build": "rimraf build && parcel build public/index.html --dist-dir build/ --no-source-maps --no-scope-hoist --no-cache --public-url . && npm list"
  },
  "dependencies": {
    "@solana/wallet-adapter-base": "0.9.22",
    "@solana/wallet-adapter-react": "0.15.32",
    "@solana/wallet-adapter-react-ui": "0.9.31",
    "@solana/wallet-adapter-wallets": "0.19.16",
    "@web3auth/base": "^1.1.1",
    "@web3auth/core": "^1.1.1",
    "@web3auth/solana-wallet-connector-plugin": "^1.1.1",
    "@web3auth/web3auth": "^1.1.1",
    "chroma-js": "2.1.1",
    "framer-motion": "10.10.0",
    "lodash": "4.17.20",
    "react-helmet-async": "1.0.7",
    "react-i18next": "11.8.5",
    "react-router-dom": "5.3.0",
    "react-router": "5.3.4",
    "history": "5.3.0"
  },
  "devDependencies": {
    "parcel": "2.0.1",
    "vercel": "28.18.3"
  }
}
